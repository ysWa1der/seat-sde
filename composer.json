{
    "name": "local-sde/seat-local-sde",
    "description": "SeAT plugin to import SDE from local CCP static data (JSONL format) instead of Fuzzwork",
    "type": "seat-plugin",
    "keywords": ["seat", "eveonline", "sde", "static-data"],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Kangtong Aideron",
            "email": "kangtong@cloudtemple.cc"
        }
    ],
    "require": {
        "php": "^8.1",
        "eveseat/services": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "LocalSde\\SeatLocalSde\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LocalSde\\SeatLocalSde\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LocalSde\\SeatLocalSde\\LocalSdeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
